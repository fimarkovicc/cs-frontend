{
  "extends": "next/core-web-vitals",
  "rules": {
    "indent": [
        "error",
        4
    ],
    "quotes": [
        "error",
        "double"
    ],
    "semi": [
        "error",
        "never"
    ],
    "no-multiple-empty-lines": [
        "error",
        {
            "max": 1
        }
    ],
    "no-console": 0
}
}
